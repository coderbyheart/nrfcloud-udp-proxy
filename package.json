{
  "name": "@coderbyheart/nrfcloud-udp-proxy",
  "version": "0.0.0-development",
  "description": "A proxy for devices that need to send data using UDP to nRF Connect for Cloud",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/proxy.js",
    "build": "npx webpack --config-name production",
    "map": "webpack-dev-server --config-name development --open",
    "lint": "eslint --ext .js,.ts,.tsx src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbyheart/nrfcloud-udp-proxy.git"
  },
  "keywords": [
    "nRF Connect for Cloud",
    "Nordic Semiconductor",
    "IoT"
  ],
  "author": "Markus Tacker <m@coderbyheart.com> | https://coderbyheart.com",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/coderbyheart/nrfcloud-udp-proxy/issues"
  },
  "homepage": "https://github.com/coderbyheart/nrfcloud-udp-proxy#readme",
  "dependencies": {
    "@types/react-dom": "^16.9.5",
    "aws-sdk": "^2.620.0",
    "chalk": "^3.0.0",
    "greenlock-express": "^4.0.3",
    "leaflet": "^1.6.0",
    "nmea-simple": "^3.0.0",
    "node-fetch": "^2.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-leaflet": "^2.6.1",
    "uuid": "^3.4.0",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@bifravst/code-style": "^6.0.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/aws-iot-device-sdk": "^2.2.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/chalk": "^2.2.0",
    "@types/node": "^13.7.1",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.20",
    "@types/react-leaflet": "^2.5.0",
    "@types/styled-components": "^4.4.3",
    "@types/uuid": "^3.4.7",
    "@types/websocket": "^1.0.0",
    "aws-iot-device-sdk": "^2.2.3",
    "forever": "^2.0.0",
    "fp-ts": "^2.5.0",
    "husky": "^4.2.3",
    "react-svg-loader": "^3.0.3",
    "source-map-loader": "^0.2.4",
    "styled-components": "^5.0.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged && npx tsc"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "src/*.ts": [
      "eslint --ext .js,.ts"
    ],
    "web/*.{ts,tsx}": [
      "eslint --ext .js,.tsx --parser-options {\"project\":\"./web/tsconfig.json\"}"
    ]
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
